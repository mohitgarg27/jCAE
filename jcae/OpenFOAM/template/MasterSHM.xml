            <Properties name="" editable="true" >     
                <Property name="mode" defVal="inside" val=""/>    
                <Property name="levels" defVal="(1E15,1)" val=""/>
            </Properties> 
        </Properties> 
         
        <Property name="locationInMesh" defVal="(0,0,0)" val=""/>
        
    </Properties>
         

	    refinementBoxCabInletJet1
	    {
	        type searchableBox;
	        min (-0.125 -0.05 -0.45);
	        max ( 0.125  1.875 0.45);
	    }
	    refinementBoxCabInletJet2
	    {
	        type searchableBox;
	        min (-0.25 -0.05 -0.48);
	        max ( 0.25  1.44 0.48);
	    }
